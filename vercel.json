{
    "version": 2,
    "builds": [
        {
            "src": "server/api/ossServer.js",
            "use": "@vercel/node"
        },
        {
            "src": "client/dist/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/oss/(.*)",
            "dest": "server/api/ossServer.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "client/dist/$1"
        }
    ]
}
